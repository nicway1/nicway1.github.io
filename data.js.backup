let bookings = [];
let favorites = [];

function loadBookings() {
    const saved = localStorage.getItem('studyspace-bookings');
    return saved ? JSON.parse(saved) : [];
}

function saveBookings(bookings) {
    localStorage.setItem('studyspace-bookings', JSON.stringify(bookings));
}

function loadFavorites() {
    const saved = localStorage.getItem('studyspace-favorites');
    return saved ? JSON.parse(saved) : [];
}

function saveFavorites(favorites) {
    localStorage.setItem('studyspace-favorites', JSON.stringify(favorites));
}

function simulateRealTimeUpdates() {
    studyLocations.forEach(location => {
        location.seats.forEach(seat => {
            if (Math.random() < 0.1) {
                seat.status = seat.status === 'available' ? 'occupied' : 'available';
            }
        });

        const availableCount = location.seats.filter(seat => seat.status === 'available').length;
        location.availableSeats = availableCount;

        const occupancyRate = (location.totalSeats - availableCount) / location.totalSeats;
        if (occupancyRate > 0.8) {
            location.crowdLevel = 'high';
        } else if (occupancyRate > 0.5) {
            location.crowdLevel = 'moderate';
        } else {
            location.crowdLevel = 'low';
        }
    });
}
